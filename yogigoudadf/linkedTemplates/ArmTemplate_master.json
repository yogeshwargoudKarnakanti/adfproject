{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "yogigoudadf"
		},
		"ls_azblob_covidreport_target_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_azblob_covidreport_target'"
		},
		"ls_azblob_covidreporting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azblob_covidreporting'"
		},
		"ls_Http_ecdc_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ecdc.blob.core.windows.net"
		},
		"ls_azblob_covidreport_target_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://yogicovidreportdl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "yogigoudadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azblob_covidreport_target_accountKey": {
						"value": "[parameters('ls_azblob_covidreport_target_accountKey')]"
					},
					"ls_azblob_covidreporting_connectionString": {
						"value": "[parameters('ls_azblob_covidreporting_connectionString')]"
					},
					"ls_Http_ecdc_data_properties_typeProperties_url": {
						"value": "[parameters('ls_Http_ecdc_data_properties_typeProperties_url')]"
					},
					"ls_azblob_covidreport_target_properties_typeProperties_url": {
						"value": "[parameters('ls_azblob_covidreport_target_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "yogigoudadf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'yogigoudadf_ArmTemplate_0')]"
			]
		}
	]
}